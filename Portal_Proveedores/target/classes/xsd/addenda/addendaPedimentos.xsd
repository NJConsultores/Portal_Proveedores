<?xml version="1.0" encoding="utf-8"?>
<xsd:schema elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="Pedimentos">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Pedimento" minOccurs="1">
          <xsd:complexType>
            <xsd:attribute name="pedimento" type="xsd:string" use="optional"/>
            <xsd:attribute name="patente" type="xsd:string" use="required"/>
            <xsd:attribute name="aduana" type="xsd:string" use="required"/>
            <xsd:attribute name="cuentaContableConceptos" type="xsd:string" use="required"/>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>