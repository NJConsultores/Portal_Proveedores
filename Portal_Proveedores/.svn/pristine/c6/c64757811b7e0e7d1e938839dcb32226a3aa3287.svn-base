package com.infra.cliente.model;

import java.io.Serializable;

import com.infra.cliente.ws.ZfeSPolist;

public class Orden implements Serializable {

	private static final long serialVersionUID = 1L;

	private ZfeSPolist orden;
	private boolean seleccionada;

	public Orden(ZfeSPolist orden, boolean seleccionada) {
		this.orden = orden;
		this.seleccionada = seleccionada;
	}

	public ZfeSPolist getOrden() {
		return orden;
	}

	public void setOrden(ZfeSPolist orden) {
		this.orden = orden;
	}

	public boolean isSeleccionada() {
		return seleccionada;
	}

	public void setSeleccionada(boolean seleccionada) {
		this.seleccionada = seleccionada;
	}

}
