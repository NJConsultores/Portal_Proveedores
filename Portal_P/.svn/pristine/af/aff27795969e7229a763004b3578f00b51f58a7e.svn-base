
package com.infra.core.model.dto;

/**
 * Clase para almacenar la informaciÃ³n de los elementos de la barra
 * @author Reyna Lagunas Aranda <reyna.lagunas.aranda@gmail.com>
 */
public class ElementoToolbar {
    private boolean puedeCrear;
    private boolean puedeEditar;
    private boolean puedeGuardar;
    private boolean puedeEliminar;

    public ElementoToolbar() {
    }

    public boolean isPuedeCrear() {
        return puedeCrear;
    }

    public void setPuedeCrear(boolean puedeCrear) {
        this.puedeCrear = puedeCrear;
    }

    public boolean isPuedeEditar() {
        return puedeEditar;
    }

    public void setPuedeEditar(boolean puedeEditar) {
        this.puedeEditar = puedeEditar;
    }

    public boolean isPuedeGuardar() {
        return puedeGuardar;
    }

    public void setPuedeGuardar(boolean puedeGuardar) {
        this.puedeGuardar = puedeGuardar;
    }

    public boolean isPuedeEliminar() {
        return puedeEliminar;
    }

    public void setPuedeEliminar(boolean puedeEliminar) {
        this.puedeEliminar = puedeEliminar;
    }

    @Override
    public String toString() {
        return "ElementoToolbar{" + "puedeNuevo=" + puedeCrear + ", puedeEditar=" + puedeEditar +
                ", puedeGuardar=" + puedeGuardar + ", puedeEliminar=" + puedeEliminar + '}';
    }
    
    
    
}
