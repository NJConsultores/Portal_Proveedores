package com.infra.cliente.model.dto;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 3/12/2014
 *
 * @author rlagunas
 */
public class LayoutClienteDto implements Serializable {

    private String noPedido;
    private String noPosicion;
    private String claseCondicion;
    private BigDecimal importe;
    private String indicadorImpuesto;
    private String textoCabecera;
    private String condicionPago;
    private String cuentaRetencion;
    private int renglon;

    
    public int getRenglon() {
		return renglon;
	}



	public void setRenglon(int renglon) {
		this.renglon = renglon;
	}



	public String getNoPedido() {
		return noPedido;
	}



	public void setNoPedido(String noPedido) {
		this.noPedido = noPedido;
	}



	public String getNoPosicion() {
		return noPosicion;
	}



	public void setNoPosicion(String noPosicion) {
		this.noPosicion = noPosicion;
	}



	public String getClaseCondicion() {
		return claseCondicion;
	}



	public void setClaseCondicion(String claseCondicion) {
		this.claseCondicion = claseCondicion;
	}



	public BigDecimal getImporte() {
		return importe;
	}



	public void setImporte(BigDecimal importe) {
		this.importe = importe;
	}



	public String getIndicadorImpuesto() {
		return indicadorImpuesto;
	}



	public void setIndicadorImpuesto(String indicadorImpuesto) {
		this.indicadorImpuesto = indicadorImpuesto;
	}



	public String getTextoCabecera() {
		return textoCabecera;
	}



	public void setTextoCabecera(String textoCabecera) {
		this.textoCabecera = textoCabecera;
	}



	public String getCondicionPago() {
		return condicionPago;
	}



	public void setCondicionPago(String condicionPago) {
		this.condicionPago = condicionPago;
	}



	public String getCuentaRetencion() {
		return cuentaRetencion;
	}



	public void setCuentaRetencion(String cuentaRetencion) {
		this.cuentaRetencion = cuentaRetencion;
	}



	@Override
    public String toString() {
        return "LayoutClienteDto{" + "noPedido=" + noPedido + ", noPosicion=" + noPosicion + ", claseCondicion=" + claseCondicion + ", importe=" + importe + ", indicadorImpuesto=" + indicadorImpuesto + ", textoCabecera=" + textoCabecera + ", condicionPago=" + condicionPago + ", cuentaRetencion=" + cuentaRetencion + '}';
    }

}
